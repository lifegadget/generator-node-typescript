{
  "name": "generator-typescript-microservice",
  "version": "5.4.3",
  "description": "A NodeJS generator for Node/TypeScript which leverages the Serverless framework to be deployable to all the major cloud platforms",
  "license": "MIT",
  "repository": "https://github.com/lifegadget/generator-typescript-microservice",
  "author": {
    "name": "Ken Snyder",
    "email": "ken@ken.net",
    "url": "https://github.com/ksnyde"
  },
  "scripts": {
    "build": "yarn sync-glob 'templates/**' 'generators/app/templates' && tsc",
    "watch": "yarn parallelshell  \"sync-glob --watch 'template/**' 'generators/app/templates'\" \"tsc -w\"",
    "watch:templates": "yarn sync-glob --watch 'templates/**' 'generators/app/templates'",
    "watch:code": "tsc -w",
    "test": "node_modules/.bin/tsc && node_modules/.bin/mocha --exit --require ts-node/register test/test-app.ts"
  },
  "keywords": [
    "yeoman-generator",
    "typescript",
    "nodejs",
    "lambda",
    "serverless",
    "aws"
  ],
  "dependencies": {
    "async-shelljs": "^0.1.4",
    "chalk": "^3.0.0",
    "common-types": "^1.14.1",
    "handlebars": "^4.7.3",
    "inquirer": "^7.1.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "simple-git": "^1.132.0",
    "sync-glob": "^1.4.0",
    "yeoman-generator": "^4.7.2",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^5.2.5",
    "@types/rimraf": "^0.0.28",
    "@types/yeoman-generator": "^3.1.4",
    "@types/yeoman-test": "^2.0.3",
    "@types/yosay": "^0.0.29",
    "chai": "^4.2.0",
    "chai-files": "^1.4.0",
    "coveralls": "^3.0.9",
    "faker": "^4.1.0",
    "lodash.first": "^3.0.0",
    "lodash.last": "^3.0.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "parallelshell": "3.0.2",
    "prettier": "^1.19.1",
    "rimraf": "^2.6.3",
    "test-console": "^1.1.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5",
    "yeoman-test": "^2.0.0"
  },
  "preferGlobal": true,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}